#include <stdio.h>
#include <stdlib.h>


int main(int argc, char *argv[]) {
	int mas [100];
	int i, a, b;
	printf ("Vvedite chislo a:");
	scanf ("%d", &a);
	printf ("Vvedite chislo b:");
	scanf ("%d", &b);
	
	srand (time (0));
	
	for (i=0; i<10; i++){
		mas [i]= (rand()%(b-a))+a;
		printf ("mas [%d] = %d\n", i, mas [i]);
		
	}
	return 0;
}
